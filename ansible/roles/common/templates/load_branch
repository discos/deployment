#! /usr/bin/env bash

export INTROOT=
export STATION=
export ACS_CDB=/alma/{{ acs_tag }}/acsdata/config/defaultCDB
export DISCOS_BRANCH=
export CDB=
BRANCHENV=/discos/{{ user }}/$DISCOS_BRANCH/.branchenv


# Load DISCOS definitions
# =======================
if [ -n "${DISCOS_BRANCH}" ] && [ -f "$BRANCHENV" ]
then
    source $BRANCHENV
fi
