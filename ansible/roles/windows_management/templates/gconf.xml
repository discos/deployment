<?xml version="1.0"?>
<gconf>
	<entry name="action" type="string">
        <stringvalue>maximize_{{ item[1].side | lower }}</stringvalue>
	</entry>
	<entry name="name" type="string">
        <stringvalue>Maximize window {{ item[1].side | lower }}</stringvalue>
	</entry>
	<entry name="binding" type="string">
        <stringvalue>&lt;Mod4&gt;{{ item[1].side }}</stringvalue>
	</entry>
</gconf>
